{
  "include": [
    "index.js",
    "managers/index.js",
    "models/index.js"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext",
    "lib": ["ESNext", "dom"],
    "resolveJsonModule": true,
    "types": ["@isomorphic-git/types"],
    "strictNullChecks": true,
    "allowJs": true,
    "declaration": true,
    "noEmit": false,
    "emitDeclarationOnly": true
  }
}
