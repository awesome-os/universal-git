{
  "include": [
    "../universal-git-src/**/*.ts",
    "../../node_modules/@awesome-os/universal-git-src/**/*.ts"
  ],
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "!node_modules/@awesome-os/**",
    "**/coverage/**"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov"
  ],
  "all": true,
  "check-coverage": false,
  "lines": 80,
  "functions": 75,
  "branches": 80,
  "statements": 80
}


